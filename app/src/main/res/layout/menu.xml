<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6a7d8a">

    <LinearLayout
        android:id="@+id/LL1"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/round_white"
        android:layout_margin="10dp">
        <LinearLayout
            android:id="@+id/test"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button10"
                android:layout_width="156dp"
                android:layout_height="55dp"
                android:layout_marginStart="30dp"
                android:background="@drawable/stroke"
                android:onClick="tests"
                android:text="Тесты"
                android:textColor="#D8D8D8" />

            <TextView
                android:id="@+id/textView39"
                android:layout_width="174dp"
                android:layout_height="57dp"
                android:layout_marginStart="10dp"
                android:gravity="center"
                android:text="Небольшие тесты по лекциям!"
                android:textColor="#011C27"
                android:textSize="18dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/dictinary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="5dp"
            android:layout_marginTop="25dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/test">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="185dp"
                android:layout_height="57dp"
                android:gravity="center"
                android:text="Краткий список важных терминов!"
                android:textColor="#011C27"
                android:textSize="18dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button"
                android:layout_width="156dp"
                android:layout_height="55dp"
                android:layout_marginStart="1dp"
                android:background="@drawable/stroke"
                android:onClick="slovar"
                android:text="Словарик"
                android:textColor="#D8D8D8"

                />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/LL2"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_margin="10dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LL1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button1"
                android:layout_width="141dp"
                android:layout_height="61dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/stroke"
                android:onClick="les1"
                android:text="1 занятие"
                android:textColor="#D8D8D8" />

            <TextView
                android:id="@+id/textView40"
                android:layout_width="117dp"
                android:layout_height="25dp"
                android:layout_marginStart="60dp"
                android:text="1. Введение"
                android:textColor="#011C27"
                android:textSize="18dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10dp"
            android:layout_marginTop="2dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/test">

            <TextView
                android:id="@+id/textView41"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-10dp"
                android:text="2. Принципы \n организации "
                android:textColor="#011C27"
                android:textSize="18dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button2"
                android:layout_width="138dp"
                android:layout_height="58dp"
                android:layout_marginStart="70dp"
                android:background="@drawable/round"
                android:backgroundTint="#013D55"
                android:onClick="les2"
                android:text="2 занятие"
                android:textColor="#D8D8D8" />
        </LinearLayout>
    </LinearLayout>




    <LinearLayout
        android:id="@+id/LL10"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LL2"
        android:layout_margin="10dp">

        <LinearLayout
            android:id="@+id/test12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button3"
                android:layout_width="141dp"
                android:layout_height="61dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/stroke"
                android:onClick="les3"
                android:text="3 занятие"
                android:textColor="#D8D8D8" />

            <TextView
                android:id="@+id/textView43"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="60dp"
                android:layout_marginTop="-15dp"
                android:text="3. Этапы \nтестирования  "
                android:textColor="#011C27"
                android:textSize="18dp" />
        </LinearLayout>

        <LinearLayout
        android:id="@+id/test1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="10dp"
        android:layout_marginTop="2dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test12">

        <TextView
            android:id="@+id/textView42"
            android:layout_width="138dp"
            android:layout_height="58dp"
            android:text="4. Жизненный цикл"
            android:layout_marginTop="-15dp"
            android:textColor="#011C27"
            android:textSize="18dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button4"
                android:layout_width="138dp"
                android:layout_height="58dp"
                android:layout_marginStart="50dp"
                android:background="@drawable/round"
                android:backgroundTint="#013D55"
                android:onClick="les4"
                android:text="4 занятие"
                android:textColor="#D8D8D8" />


        </LinearLayout>
</LinearLayout>











    <LinearLayout
        android:id="@+id/LL11"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LL10"
        android:layout_margin="10dp">

    <LinearLayout
        android:id="@+id/test15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button5"
        android:layout_width="141dp"
        android:layout_height="61dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/round"
        android:backgroundTint="#013D55"
        android:onClick="les5"
        android:text="5 занятие"
        android:textColor="#D8D8D8" />

        <TextView
            android:id="@+id/textView44"
            android:layout_width="181dp"
            android:layout_height="46dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="-15dp"
            android:text="5. Критерии \n тестирования"
            android:textColor="#011C27"
            android:textSize="18dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/test13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <TextView
            android:id="@+id/textView45"
            android:layout_width="170dp"
            android:layout_height="58dp"
            android:text="6.Классификация тестов"
            android:textColor="#011C27"
            android:textSize="18dp"
            android:layout_marginTop="-15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button6"
        android:layout_width="138dp"
        android:layout_height="58dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/round"
        android:backgroundTint="#013D55"
        android:onClick="les6"
        android:text="6 занятие"
        android:textColor="#D8D8D8" />


    </LinearLayout>
</LinearLayout>




    <LinearLayout
        android:id="@+id/LL12"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LL11"
        android:layout_margin="10dp">

    <LinearLayout
        android:id="@+id/test17"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button7"
        android:layout_width="141dp"
        android:layout_height="61dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/round"
        android:backgroundTint="#013D55"
        android:onClick="les7"
        android:text="7 занятие"
        android:textColor="#D8D8D8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="117dp"
        android:layout_height="25dp"
        android:layout_marginStart="60dp"
        android:text="7. Виды тестирования"
        android:textColor="#011C27"
        android:textSize="18dp"
        android:layout_marginTop="-1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/test16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView60"
            android:layout_width="170dp"
            android:layout_height="58dp"
            android:layout_marginStart="10dp"
            android:text="8. Тест-план"
            android:textColor="#011C27"
            android:textSize="18dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button8"
        android:layout_width="138dp"
        android:layout_height="58dp"
        android:layout_marginStart="25dp"
        android:background="@drawable/round"
        android:backgroundTint="#013D55"
        android:onClick="les8"
        android:text="8 занятие"
        android:textColor="#D8D8D8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    </LinearLayout>
</LinearLayout>


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="98dp"
        android:layout_height="73dp"
        android:layout_marginStart="336dp"
        android:layout_marginTop="228dp"
        android:rotation="-91"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cat_stand" />




</androidx.constraintlayout.widget.ConstraintLayout>