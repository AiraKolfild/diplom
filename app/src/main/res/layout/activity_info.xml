<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6a7d8a"
    tools:context=".menutests">

    <LinearLayout
        android:id="@+id/LL5"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_margin="10dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/round_white"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/test"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button10"
                android:layout_width="156dp"
                android:layout_height="55dp"
                android:layout_marginStart="30dp"
                android:background="@drawable/stroke"
                android:onClick="home"
                android:text="Меню"
                android:textColor="#D8D8D8" />

            <TextView
                android:id="@+id/textView39"
                android:layout_width="174dp"
                android:layout_height="57dp"
                android:layout_marginStart="10dp"
                android:gravity="center"
                android:text="В главное меню"
                android:textColor="#011C27"
                android:textSize="18dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/dictinary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="5dp"
            android:layout_marginTop="25dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/test">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="185dp"
                android:layout_height="57dp"
                android:gravity="center"
                android:text="Краткий список важных терминов!"
                android:textColor="#011C27"
                android:textSize="18dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button"
                android:layout_width="156dp"
                android:layout_height="55dp"
                android:layout_marginStart="1dp"
                android:background="@drawable/stroke"
                android:onClick="slovar"
                android:text="Словарик"
                android:textColor="#D8D8D8"

                />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/LL1"
        android:layout_width="394dp"
        android:layout_height="471dp"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="60dp"
        android:layout_marginBottom="32dp"
        android:orientation="vertical"
        android:paddingStart="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button120"
            android:layout_width="329dp"
            android:layout_height="67dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/round"
            android:onClick="url1"
            android:text="Книга «Тестирование программного обеспечения. Базовый курс.»"
            android:textColor="#D8D8D8" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button130"
            android:layout_width="329dp"
            android:layout_height="67dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/round"
            android:onClick="url2"
            android:text="Большой учебник по тестированию"
            android:textColor="#D8D8D8" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button110"
            android:layout_width="329dp"
            android:layout_height="67dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/round"
            android:onClick="url3"
            android:text="Рекс Блэк. «Ключевые процессы тестирования»"
            android:textColor="#D8D8D8" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button9"
            android:layout_width="329dp"
            android:layout_height="67dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/round"
            android:onClick="url4"
            android:text="Гибкое тестирование. Практическое руководство для тестировщиков ПО и гибких команд"
            android:textColor="#D8D8D8" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button15"
            android:layout_width="329dp"
            android:layout_height="67dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="464dp"
            android:background="@drawable/round"
            android:onClick="url5"
            android:text="Гленфорд Майерс, Том Баджетт,Кори Сандлер «Искусство тестирования программ.»"
            android:textColor="#D8D8D8" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>